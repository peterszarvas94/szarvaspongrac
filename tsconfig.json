{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "pocketbase": [
        "./public/js/pocketbase/pocketbase@0.26.3.es.d.ts"
      ],
      "content-manager": [
        "./public/js/content-manager.js"
      ],
      "auth": [
        "./public/js/auth.js"
      ],
      "env": [
        "./public/js/env.js"
      ],
      "gallery": [
        "public/js/gallery.js"
      ],
      "popover": [
        "public/js/popover.js"
      ],
    }
  },
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "**/*.js"
  ],
  "exclude": [
    "dist"
  ]
}
