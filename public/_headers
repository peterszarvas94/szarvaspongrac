# Cache images for 1 year
/images/*
  Cache-Control: public, max-age=31536000, immutable

# Cache fonts for 1 year  
/fonts/*
  Cache-Control: public, max-age=31536000, immutable

# Cache other static assets for 1 month
/*.css
  Cache-Control: public, max-age=2592000

/*.js
  Cache-Control: public, max-age=2592000